{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
      "typedef": [false, "call-signature", "parameter", "member-variable-declaration"],
      "no-inferrable-types": [true, "ignore-params", "ignore-properties"]
    }
}
